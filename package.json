  "scripts": {
    "start": "docker compose up",
    "start:prod": "rollup -c src/server/rollup.config.js && NODE_ENV=production node server.js",
    "start:test": "rollup -c src/server/rollup.config.js && NODE_ENV=test node server.js",
    "start:dev": "NODE_ENV=development concurrently \"npm run start:dev:server\" \"npm run start:dev:front\"",
    "start:dev:front": "vite",
    "start:dev:server": "rollup -c -w src/server/rollup.config.js && node server.js",
    "test": "npx playwright test",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write '*.{ts,tsx,json,css,md}'",
    "build": "DISABLE_ESLINT_PLUGIN=true vite build",
    "prepare": "husky install"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "airbnb-typescript",
      "prettier",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
      "react",
      "@typescript-eslint",
      "react-hooks",
      "prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "project": "./tsconfig.json"
    },
    "rules": {
      "react/function-component-definition": [
        2,
        {
          "namedComponents": "arrow-function",
          "unnamedComponents": "arrow-function"
        }
      ],
      "react/jsx-props-no-spreading": "off",
      "import/no-cycle": "off",
      "@typescript-eslint/no-explicit-any": "off"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint",
    "*.{ts,tsx,json,css,md}": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@playwright/test": "^1.40.1",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/connect-redis": "^0.0.23",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jsdom": "^21.1.3",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.0.0",
    "@types/passport": "^1.0.12",
    "@types/prettier": "^2.7.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "concurrently": "^7.6.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "rollup": "^4.29.1",
    "typescript": "^4.9.4"
  },
  "proxy": "http://localhost:8000"
}
